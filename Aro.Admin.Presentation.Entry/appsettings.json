{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "sqlConnection": "Server=.;Database=AroAdminDb1;Trusted_Connection=True;TrustServerCertificate=True;"
  },
  "Cors": {
    "Policies": [
      {
        "Name": "DevelopmentCors",
        "AllowedOrigins": [ "*" ],
        "AllowedMethods": [ "GET", "POST", "PUT", "DELETE" ],
        "AllowedHeaders": [ "*" ],
        "ExposedHeaders": [ "X-Pagination" ]
      }
    ]
  },
  "EnableEfCoreParameterValuesLogging": true,
  "AdminSettings": {
    "AdminRoleName": "SystemAdmin"
  }
}
